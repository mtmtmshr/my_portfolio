    "autoprefixer": "^10.3.3",
    "axios": "^0.21.1",
    "msw": "^0.35.0",
    "next": "11.1.2",
    "next-page-tester": "^0.28.0",
    "postcss": "^8.3.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "swr": "^1.0.0",
    "tailwindcss": "^2.2.9"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.2.0",
    "@testing-library/jest-dom": "^5.14.1",
